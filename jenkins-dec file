pipeline{
  agent any
environment{
PATH="/var/lib/jenkins/tools/hudson.tasks.Maven_MavenInstallation/maven3.9.4/bin:$PATH"
}
    stages{
    stage("git integration"){
      steps{
         git credentialsId: 'ba27e6d1-8dac-4b51-af4e-007421fac11f', url: 'https://github.com/asma9030994/maven-web-application.git'
    }
    }
stage("build"){
steps{
  sh "mvn clean install package"
}
}
stage("test"){
steps{
withSonarQubeEnv("sonar"){
sh "mvn sonar:sonar"
}
}
}
}
}





             
  
